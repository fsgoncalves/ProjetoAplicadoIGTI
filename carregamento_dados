FROM 'D:\_PÓS DATA ANALYTICS\base_dados_pa\teste\dadosSaque202102.csv'
WITH (FIRSTROW = 2,
    FIELDTERMINATOR = ';',
    ROWTERMINATOR='\n',
    BATCHSIZE=250000,
    MAXERRORS=2);
